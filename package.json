{
    "name": "ptn",
    "displayName": "Plutonium",
    "publisher": "plutonium",
    "description": "Rich Plutonium language support for Visual Studio Code",
    "icon": "img/plutonium.png",
    "version": "0.0.4",
    "repository": {
      "type": "git",
      "url": "https://github.com/Plutonium-Lang/vscode-plutonium.git"
    },
    "engines": {
      "vscode": "^1.80.0"
    },
    "categories": [
      "Programming Languages",
      "Snippets",
      "Linters"
    ],
    "contributes": {
      "languages": [
        {
          "id": "ptn",
          "aliases": [
            "plutonium",
            "ptn"
          ],
          "extensions": [
            ".ptn"
          ],
          "configuration": "./language-configuration.json"
        }
      ],
      "grammars": [
        {
          "language": "ptn",
          "scopeName": "source.ptn",
          "path": "./syntaxes/ptn.tmLanguage.json"
        }
      ]
    }
  }