{
    "name": "calyxium",
    "displayName": "Calyxium",
    "publisher": "Calyxium",
    "description": "Rich Calyxium language support for Visual Studio Code",
    "icon": "img/calyxium.jpg",
    "version": "0.0.6",
    "repository": {
      "type": "git",
      "url": "https://github.com/Calyxium/vscode-calyxium.git"
    },
    "engines": {
      "vscode": "^1.80.0"
    },
    "categories": [
      "Programming Languages",
      "Snippets",
      "Linters"
    ],
    "contributes": {
      "languages": [
        {
          "id": "cx",
          "aliases": [
            "calyxium",
            "cx"
          ],
          "extensions": [
            ".cx"
          ],
          "configuration": "./language-configuration.json"
        }
      ],
      "grammars": [
        {
          "language": "cx",
          "scopeName": "source.cx",
          "path": "./syntaxes/cx.tmLanguage.json"
        }
      ]
    }
  }
